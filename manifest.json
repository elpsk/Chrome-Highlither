{
	"name": "Chrome Highlither",
	"description": "Highlith all array words on page!",
	"version": "0.1",
	"permissions": ["contextMenus", "tabs"],
	"icons": { "16" : "icon.png", "48" : "icon.png", "128" : "icon.png", "256" : "icon.png" },
	"background_page": "background.html",
	
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Chrome Highlither",
    "default_popup": "popup.html"
  },


  "content_scripts": [{
      "matches": ["http://*/*", "https://*/*"],
      "js": ["contentScript.js"]
  }]


}